def is_prime(N):
    N += 1
    C = [True] * N
    C[0] = C[1] = False
    for k in range(2, int(N**0.5)):
        if C[k]:
            for m in range(2*k, N, k):
                C[m] = False
    return [k for k in range(N) if C[k]]


