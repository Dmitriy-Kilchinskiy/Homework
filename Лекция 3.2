
def nod(number1, number2):
    del1 = [number1]
    del2 = [number2]
    for i in range(1, number1//2):
        if number1 % i == 0:
            del1.append(i)
    for i in range(1, number2//2):
        if number2 % i == 0:
            del2.append(i)
    a = 1
    for i in del1:
        for m in del2:
            if i == m and i > a:
                a = i
    return a


